@model List<ProjectSS.Web.Models.CRMCallHistoryModel>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-sm-6">
                        <h4>Call History</h4>
                    </div>
                    <div class="col-sm-6">
                        <div class="pull-right" style="padding-top:10px">
                            <button type="button" data-toggle="modal" data-target="#callmodal" class="btn btn-success btn-xs">New Entry</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel-body">
                <table id="datatable1" class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <th>Date</th>
                            <th>Time</th>
                            <th>Remarks</th>
                            <th>Caller</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model != null)
                        {
                            foreach (var u in Model)
                            {
                                <tr>
                                    <td>@u.Date</td>
                                    <td>@u.Time</td>
                                    <td>@u.Remarks</td>
                                    <td>@u.Caller</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>