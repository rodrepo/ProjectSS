@model ProjectSS.Web.Models.CRMCallHistoryModel
@Html.HiddenFor(m => m.CRMId)
<div class="row">
    <div class="col-lg-12">
        <div class="form-group">
            <label></label>
            @Html.TextAreaFor(model => model.Remarks, new { @class = "form-control", @type = "text", @required = "required", @row = "4", @placeholder = "Enter Remarks" })
            @Html.ValidationMessageFor(model => model.Remarks, "", new { @class = "text-danger" })
        </div>
    </div>
</div>
